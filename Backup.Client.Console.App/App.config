<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <sectionExtension type="Microsoft.Practices.Unity.InterceptionExtension.Configuration.InterceptionConfigurationExtension, Microsoft.Practices.Unity.Interception.Configuration" />
    <container>
      <extension type="Microsoft.Practices.Unity.InterceptionExtension.Interception, Microsoft.Practices.Unity.Interception" />
      <interception/>
      <register type="Backup.Common.Logger.ILogger, Backup.Common" mapTo="Backup.Client.Console.App.ConsoleLogger, Backup.Client.Console.App" />
      <register type="Backup.Client.BL.Interfaces.IBackupStrategy, Backup.Client.BL" mapTo="Backup.Client.BL.BackupLogic.BackupStrategyCopyFiles, Backup.Client.BL"/>
      <register type="Backup.Client.BL.Interfaces.IActivityFacade, Backup.Client.BL" mapTo="Backup.Client.BL.Facades.ActivityFacade, Backup.Client.BL" />
      <register type="Backup.Client.BL.Interfaces.IBackupFacade, Backup.Client.BL" mapTo="Backup.Client.BL.Facades.BackupFacade, Backup.Client.BL" />
      <register type="Backup.Client.BL.Interfaces.IBackupController, Backup.Client.BL" mapTo="Backup.Client.BL.BackupLogic.BackupController, Backup.Client.BL">
        <interceptor type="InterfaceInterceptor"/>
        <interceptionBehavior type="Backup.Client.BL.IoC.ExceptionInterceptionBehavior, Backup.Client.BL" isDefaultForType="true"/>
        <lifetime type="singleton" />
        <constructor>
           <param name="timerInterval" value="5000" />
        </constructor>
      </register>
      
    </container>
  </unity>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Practices.ServiceLocation" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.3.0.0" newVersion="1.3.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>